{{! -*- mode: web; web-mode-markup-indent-offset: 2 -*- }}
<table class="datatable">
  <thead>
    <tr>
      <th>{{#str}}username, tool_datatables{{/str}}</th>
      <th>first name</th>
      <th>last name</th>
      <th>last access</th>
      <th>auth</th>
    </tr>
  </thead>
  <tbody>
    {{#users}}
      <tr>
	<td>{{username}}</td>
	<td>{{firstname}}</td>
	<td>{{lastname}}</td>
	<td>{{lastaccess_str}}</td>
	<td>{{auth}}</td>
      </tr>
    {{/users}}
  </tbody>
</table>

