<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/sitereport/db" VERSION="20140120" COMMENT="XMLDB file for Moodle blocks/sitereport"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_course_creator" COMMENT="Course creator">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="user id"/>             
        <FIELD NAME="courseid" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false" />
        <FIELD NAME="timecreated" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" />
     </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
     
  </TABLES>
</XMLDB>